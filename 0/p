FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

norm_pres		800.9e2 ;		// 6Â°C 845hPa 1,055kg/m3

internalField   uniform $norm_pres;

boundaryField
{
	"symmetry.*"
	{
		type	symmetry;
	}
	"ballute.*"
	{
		type	zeroGradient;
	}

    "sides.*"
    {
        type            zeroGradient;
    }

    "inlet.*"
    {
        type            zeroGradient;
    }

    "outlet.*"
    {
        type            fixedValue;
        value           uniform $norm_pres;
    }

	"emtpy.*"
	{
		type		empty;
	}
}

