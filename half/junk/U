FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	"symmetry.*"
	{
		type	symmetry;
	}

	"ballute.*"
	{
		type	noSlip;
	}

    "sides.*"
    {
        type            slip;
    }

    "inlet.*"
    {
        type            fixedValue;
        value           uniform (0 29.7 0);
    }

    "outlet.*"
    {
        type            zeroGradient;
    }

}

