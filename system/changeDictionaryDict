FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundary
{
    inlet 
    {
        type        inlet;
    }
    outlet
    {
        type        outlet;
    }
    top 
    {
        type	    empty;
    }
    bottom 
    {
        type	    empty;
    }
	sides
	{
		type		wall;
	}
	ballute
	{
		type		wall;
	}
}


